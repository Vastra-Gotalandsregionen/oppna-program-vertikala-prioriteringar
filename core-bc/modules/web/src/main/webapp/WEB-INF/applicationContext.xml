  <bean name="diagnosKodRepository" class="se.vgregion.verticalprio.repository.JpaDiagnosKodRepository" />
  <bean name="haelsonekonomiskEvidensKodRepository" class="se.vgregion.verticalprio.repository.JpaHaelsonekonomiskEvidensKodRepository" />
  <bean name="kostnadLevnadsaarKodRepository" class="se.vgregion.verticalprio.repository.JpaKostnadLevnadsaarKodRepository" />
  <bean name="patientnyttaEffektAatgaerdsKodRepository" class="se.vgregion.verticalprio.repository.JpaPatientnyttaEffektAatgaerdsKodRepository" />
  <bean name="patientnyttoEvidensKodRepository" class="se.vgregion.verticalprio.repository.JpaPatientnyttoEvidensKodRepository" />
  <bean name="rangordningsKodRepository" class="se.vgregion.verticalprio.repository.JpaRangordningsKodRepository" />
  <bean name="tillstaandetsSvaarighetsgradKodRepository" class="se.vgregion.verticalprio.repository.JpaTillstaandetsSvaarighetsgradKodRepository" />
  <bean name="vaardformsKodRepository" class="se.vgregion.verticalprio.repository.JpaVaardformsKodRepository" />
  <bean name="vaardnivaaKodRepository" class="se.vgregion.verticalprio.repository.JpaVaardnivaaKodRepository" />
  <bean name="vaentetidsKodRepository" class="se.vgregion.verticalprio.repository.JpaVaentetidsKodRepository" />
  
</beans>
