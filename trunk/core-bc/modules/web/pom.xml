    <plugins>
      <plugin>
        <groupId>org.mortbay.jetty</groupId>
        <artifactId>maven-jetty-plugin</artifactId>
        <version>6.1.8</version>
        <configuration>
          <contextPath>/vp</contextPath>
          <scanIntervalSeconds>1</scanIntervalSeconds>
          <connectors>
