<persistence xmlns="http://java.sun.com/xml/ns/persistence"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://java.sun.com/xml/ns/persistence http://java.sun.com/xml/ns/persistence/persistence_2_0.xsd"
        version="2.0">

  <persistence-unit name="vertikal">
    <provider>org.hibernate.ejb.HibernatePersistence</provider>
    <class>se.vgregion.verticalprio.entity.User</class>
    <class>se.vgregion.verticalprio.entity.Prioriteringsobjekt</class>
    <class>se.vgregion.verticalprio.entity.PrioriteringsobjektUtkast</class>
    <class>se.vgregion.verticalprio.entity.DiagnosKod</class>
    <class>se.vgregion.verticalprio.entity.AtcKod</class>
    <class>se.vgregion.verticalprio.entity.SektorRaad</class>
    <class>se.vgregion.verticalprio.entity.TillstaandetsSvaarighetsgradKod</class>
    <class>se.vgregion.verticalprio.entity.VaardformsKod</class>
    <class>se.vgregion.verticalprio.entity.AatgaerdsKod</class>
    <class>se.vgregion.verticalprio.entity.VaentetidsKod</class>
    <class>se.vgregion.verticalprio.entity.HaelsonekonomiskEvidensKod</class>
    <class>se.vgregion.verticalprio.entity.VaardnivaaKod</class>
    <class>se.vgregion.verticalprio.entity.RangordningsKod</class>
    <class>se.vgregion.verticalprio.entity.AatgaerdsRiskKod</class>
    <class>se.vgregion.verticalprio.entity.PatientnyttoEvidensKod</class>
    <class>se.vgregion.verticalprio.entity.PatientnyttaEffektAatgaerdsKod</class>
    <class>se.vgregion.verticalprio.entity.KostnadLevnadsaarKod</class>
    
    <shared-cache-mode>ENABLE_SELECTIVE</shared-cache-mode>
    <properties>
      <property name="hibernate.cache.provider_class" value="org.hibernate.cache.SingletonEhCacheProvider"/>
      <property name="hibernate.cache.user_second_level" value="true"/>
      <property name="hibernate.cache.user_query_cache" value="true"/>      
    </properties>
    
  </persistence-unit>

</persistence>
